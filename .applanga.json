{
  "app": {
    "pull": {
      "target": [
        {
          "file_format": "react_nested_json",
	  "tag": "react_strings",
          "path": "./lang/<language>.json",
          "includeInvisibleId": true
        }
      ]
    }, 
    "push": {
      "source": [
        {
          "language": "en",
          "file_format": "react_nested_json",
	  "tag": "react_strings",
          "path": "./lang/en.json"
        }
      ]
    }
  }
}
